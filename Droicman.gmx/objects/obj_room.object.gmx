        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Control sounds
if global.game_ended == 0
{
    if global.player_death == 0 &amp;&amp; !audio_is_playing(snd_bgm) //Plays BGM
    {
       audio_play_sound(snd_bgm, 1, 1);
    }
    
    if instance_number(obj_enemy) == 0 //BGM without enemies
    {
        audio_sound_pitch(snd_bgm, 1);
    }
       
    if instance_number(obj_enemy) &gt; 0 &amp;&amp; obj_enemy.player_invincible &gt; 0  //Invincibility
    {
        audio_sound_pitch(snd_bgm, 1.5);
    }
    
    if instance_number(obj_enemy) &gt; 0 &amp;&amp; obj_enemy.player_invincible == 0  //BGM without invincibility
    {
        audio_sound_pitch(snd_bgm, 1);
    }
    
    /*if audio_is_playing(snd_chom)
    {
        audio_sound_gain(snd_bgm, 0, 0);
    }
    else
    {
        audio_sound_gain(snd_bgm, 1, 0);
    }*/
    
    //Stopping one-shot sounds when game starts
    if audio_is_playing(snd_title) 
    {
        audio_stop_sound(snd_title);
    }
    if audio_is_playing(snd_win)
    {
        audio_stop_sound(snd_win);
    }
    if audio_is_playing(snd_loss)
    {
        audio_stop_sound(snd_loss);
    }
    
}

if global.player_death == 1 &amp;&amp; audio_is_playing(snd_bgm) //Stop BGM
{
    audio_stop_sound(snd_bgm);
}
