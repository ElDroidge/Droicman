        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw HUD; draw stats upon ending of game

//Highscore display during Welcome screen
if global.game_ended == -1
{
    draw_text(room_width / 2, 80, "DOROIJU INDUSTRIES");
    draw_text(room_width / 2, 96, "PRESENTS");
    draw_text(room_width / 2, 128, "DROICMAN MODEL DG-16");
    draw_text(room_width / 2, 208, "HISCORE " + string(highscore) );
    draw_text(room_width / 2, 240, "PRESS DPAD UP");
    draw_text(room_width / 2, 256, "TO START");
    draw_text(room_width / 2, 305, "V0.2.2");
}

//In-game HUD
if global.game_ended == 0
{
    draw_text(64, 28, global.game_level); //Level
    draw_text(room_width / 2, 28, global.player_score); //Score
    draw_text(room_width - 58, 28, game_timer); //Game timer
}

//Game Over display
if global.game_ended == 1
{
    draw_text(room_width / 2, 64, "GAME OVER");
    draw_text(room_width / 2, 112, "POINTS " + string(global.player_score) );
    draw_text(room_width / 2, 128, "- TIME " + string(game_timer) );
    draw_text(room_width / 2, 144, "----------------");
    draw_text(room_width / 2, 160, "TOTAL " + string(global.player_score - game_timer) );
    if highscore_opened == 1 &amp;&amp; score_new &gt; score_old
    {
        draw_text(room_width / 2, 192, "NEW HISCORE");
    }
    draw_text(room_width / 2, 240, "PRESS DPAD UP");
    draw_text(room_width / 2, 256, "TO RESTART");
}

</string>
